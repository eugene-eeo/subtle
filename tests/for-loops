let x = {};
for (i in 1..5) {
    if (i == 2) continue;
    if (i == 5) break;
    x.setOwnSlot(i, Fn.new{ return i; });
}

# assert x.getOwnSlot(1).call == 1;
# assert !x.hasOwnSlot(2);
# assert x.getOwnSlot(3).call == 3;
# assert x.getOwnSlot(4).call == 4;
# assert !x.hasOwnSlot(5);
