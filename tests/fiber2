let f = Fiber.current;
# f.parent.println;
let g = f.call;
# f.parent.println;
# f.println;
# f.yield;
g.println;

let get_fiber = Fn.new{
    return Fiber.new{|v|
        while (true) {
            v println;
            let s = Fiber.yield(v);
            if (!s) break;
            v = v + 1;
        }
    };
};

let f = get_fiber.call;
f.call(1);
f.call(true);
f.call(true);

let x = {};
let f = Fiber.new{
    x.setSlot = Fn.new{|k, v|
        k.println;
        let rv = Fiber.yield;
        x.setOwnSlot(k, v);
        return rv;
    };
    assert (x.b = 1) == 2;
    assert x.b == 1;
};
f.call;
assert !x.hasOwnSlot("b");
f.call(2);

