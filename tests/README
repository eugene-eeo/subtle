Point := {}
Point :: new-x: x y: y [
    p := Point clone
    p x = x
    p y = y
    p
]
Point :: + other [
    Point new-x: (self x + other x) y: (self y + other y)
]
p1 := Point new-x: 1 y: 2
p2 := Point new-x: 3 y: 4
result := p1 + p2
assert: (result x == 4) msg: "!"
assert: (result y == 6) msg: "!"
