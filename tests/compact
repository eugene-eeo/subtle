# test table compaction.
let x = {};
let i;
let MAX = 4096;

i = 0;
while (i < MAX) {
    x.setSlot(i, i);
    i = i + 1;
    let j = 0;
    while (j < i) {
        assert x.getSlot(j) == j;
        j = j + 1;
    }
}

i = 0;
while (i < MAX) {
    x.deleteSlot(i);
    i = i + 1;
    let j = MAX - 1;
    while (j > i) {
        assert x.getSlot(j) == j;
        j = j - 1;
    }
}
