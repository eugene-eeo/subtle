assert 1 + 2 == 3;
assert 1 / 2 == 0.5;
assert 3 - 2 == 1;
assert 5 * 2 == 10;
assert -1 == 1 - 2;
assert (1 | 2) == 3;
assert (1 & 2) == 0;

assert 0;
assert !!1;
assert !nil;
assert !false;
assert !0 == false;
assert !nil == true;

assert "a" + "b" == "ab";
assert "ab" == "ab";
assert (1 + 2 == 3) == true;
assert true == true;
assert nil == nil;

assert nil != true;
assert true != false;
assert 0 != 1;
assert "a" != "b";
