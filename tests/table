# tests objects (specifically, the table implementation)

let x = {};
x.setSlot("abc", 1);
x.setSlot("def", 2);

assert x.getSlot("abc") == 1;
assert x.getSlot("def") == 2;
assert x.abc == 1;
assert x.def == 2;

assert !x.deleteSlot("ab");
assert x.deleteSlot("abc");
assert x.ab == nil;
assert x.abc == nil;
assert !x.hasOwnSlot("abc");

x.setSlot("ghi", 3);
x.setSlot("jkl", 4);
assert x.def == 2;
assert x.ghi == 3;
assert x.jkl == 4;
